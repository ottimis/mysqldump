{
    "name": "@ottimis/mysqldump",
    "version": "4.0.0",
    "description": "Create a DUMP from MySQL and return observable",
    "main": "dist/cjs.js",
    "module": "dist/es.js",
    "typings": "dist/mysqldump.d.ts",
    "scripts": {
        "build": "rollup -c && dts-bundle-generator -o dist/mysqldump.d.ts build-decl/main.d.ts --umd-module-name=mysqldump && rimraf build-decl",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ottimis/mysqldump"
    },
    "keywords": [
        "backup",
        "mysql",
        "mysql-dump",
        "dump",
        "restore",
        "database",
        "backup",
        "database"
    ],
    "license": "MIT",
    "dependencies": {
        "deepmerge": "^3.2.0",
        "mysql2": "^2.3.3",
        "rxjs": "^7.4.0",
        "sql-formatter": "^4.0.2",
        "sqlstring": "^2.3.2"
    },
    "devDependencies": {
        "@types/node": "^16.11.9",
        "@types/sqlstring": "^2.3.0",
        "dts-bundle-generator": "^6.0.0",
        "prettier": "^2.4.1",
        "rimraf": "^2.7.1",
        "rollup-plugin-typescript2": "^0.31.0",
        "tmp": "^0.1.0",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.4.4"
    }
}
